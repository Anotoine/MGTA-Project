function [Slots] = computeSlotsGDP (HStart, Hend, HNoReg, PAAR, AAR)
    Slots(:,1) = [HStart(1):1/PAAR:Hend(1) (Hend(1)+(1/PAAR)):1/AAR:HNoReg(1)];    
    Slots(:,3) = zeros(1,length(Slots));
    for i = 1:length(Slots)
        Slots(i,2) = Slots(i,1);
        Slots(i,1) = int8(Slots(i,1));
    end
    SlotsA(:,2) = int8([HStart(2)*60:(1/PAAR)*60:Hend(2)*60 (Hend(2)*60)+1:(1/AAR)*60:HNoReg(2)*60]);

    

    
    
end